{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "jsx": "preserve",
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "paths": {
      "@site/*": [
        "./*"
      ],
      "@docComponents/*": [
        "./src/components/*"
      ],
      "@docPages/*": [
        "./src/pages/*"
      ],
      "@docViews/*": [
        "./src/views/*"
      ],
      "@menu/*": [
        "./src/@menu/*"
      ],

      "@/*": [
        "../typescript-version/full-version/src/*"
      ],
      "@core/*": [
        "../typescript-version/full-version/src/@core/*"
      ],
      "@layouts/*": [
        "../typescript-version/full-version/src/@layouts/*"
      ],
      "@assets/*": [
        "../typescript-version/full-version/src/assets/*"
      ],
      "@components/*": [
        "../typescript-version/full-version/src/components/*"
      ],
      "@configs/*": [
        "../typescript-version/full-version/src/configs/*"
      ],
      "@views/*": [
        "../typescript-version/full-version/src/views/*"
      ]
    }
  }
}
